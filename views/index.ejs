<!DOCTYPE html>
<html lang="en" ng-app="myTimer">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Online Timer with HTML5 Notifications</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/bootstrap/dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="/styles/styles.css">
        <link href="/images/timer15.png" rel="shortcut icon" type="image/vnd.microsoft.icon">

    </head>
    <body ng-controller="MainCtlr" ng-cloak class="ng-cloak">
      <!-- tell people to get with the javascript program -->
      <noscript>You should get some javascript!</noscript>

      <!-- main dom -->
      <div class="container">

        <!-- header -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3 header">
            <h1><img src="/images/timer15.png"/> HTML5 Timer and Notification</h1>
          </div>
        </div>

        <!-- tabs -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3">

            <!-- TABSSET -->
            <div tabset justified="true">
              <!-- TIMER TAB -->
              <div tab heading="Timer">
                <div timer-section></div>
              </div>
              <!-- CUSTOMIZE TAB -->
              <div tab heading="Customize">
                <div customize-notif></div>
              </div>
              <!-- PERMISSIONS TAB -->
              <div tab heading="Permissions">
                <div permissions-tab></div>
              </div>
            </div><!-- end of <div tabset justified="true"> -->

          </div><!-- <div class="col-md-6 col-md-offset-3"> -->
        </div><!-- <div class="row"> -->

      </div><!-- end of .container -->

      <!-- javascript -->
      <script type="text/javascript" src="/angular/angular.min.js"></script>
      <script type="text/javascript" src="/moment/min/moment.min.js"></script>
      <script type="text/javascript" src="/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
      <script type="text/javascript" src="/angular-timer/dist/angular-timer.min.js"></script>
      <!-- my angular -->
      <script type="text/javascript" src="/angular/main.js"></script>
      <!-- filters -->
      <script type="text/javascript" src="/angular/filters/ms-to-time-string.js"></script>
      <!-- services -->
      <script type="text/javascript" src="/angular/services/notifications.js"></script>
      <script type="text/javascript" src="/angular/services/time.js"></script>
      <!-- directives -->
      <script type="text/javascript" src="/angular/directives/timer-section.js"></script>
      <script type="text/javascript" src="/angular/directives/customize-notif.js"></script>
      <script type="text/javascript" src="/angular/directives/permissions-directive.js"></script>
      <!-- controllers -->
      <script type="text/javascript" src="/angular/controllers/main-controller.js"></script>
    </body>
</html>
